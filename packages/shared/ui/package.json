{
    "name": "@dakan/ui",
    "version": "0.0.4",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/dakan"
    },
    "scripts": {
        "check-types": "tsc",
        "build": "yarn rimraf dist && babel src -d dist",
        "start": "yarn rimraf dist && babel -w src -d dist",
        "build-docs": "build-storybook --docs",
        "coverage": "jest --coverage",
        "storybook:build": "build-storybook",
        "storybook:deploy": "storybook-to-ghpages",
        "storybook": "start-storybook -p 6006",
        "docs": "cd docs && yarn start",
        "format": "prettier --write \"./src/**/*.{js,json,md,yml,ts,tsx,jsx}\"",
        "test": "yarn build && yarn lint && yarn coverage",
        "unit": "jest",
        "publish:public": "yarn run build && npm publish --access=public"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.7.7",
        "@storybook/addon-a11y": "^6.0.5",
        "@storybook/addon-actions": "^6.0.5",
        "@storybook/addon-docs": "^6.0.5",
        "@storybook/addon-knobs": "^6.0.5",
        "@storybook/addon-storysource": "^6.0.5",
        "@storybook/addons": "^6.0.5",
        "@storybook/react": "^6.0.5",
        "@storybook/source-loader": "^6.0.5",
        "@storybook/storybook-deployer": "^2.8.1",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.6",
        "jest": "^26.4.0",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "@beam-australia/react-env": "^2.1.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "@dakan/theme": "^0.0.4",
        "amplitude-js": "^7.1.0",
        "axios": "^0.20.0",
        "baseui": "^9.89.0",
        "deepmerge": "^4.2.2",
        "js-cookie": "^2.2.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-markdown": "^4.3.1",
        "react-virtualized-auto-sizer": "^1.0.2",
        "shortid": "^2.2.15",
        "styled-components": "^4.4.0",
        "styletron-react": "^5.2.6"
    }
}
