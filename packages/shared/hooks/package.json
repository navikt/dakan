{
  "name": "@dakan/hooks",
  "version": "0.0.10",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/dakan"
  },
  "scripts": {
    "check-types": "tsc",
    "build": "yarn rimraf dist && babel src -d dist",
